{
  "name": "elsa-quiz-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "start": "nest start",
    "start:dev": "export CDC_ENV=dev || set CDC_ENV=dev&& nest start --watch",
    "start:debug": "export CDC_ENV=dev || set CDC_ENV=dev&& nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint-output \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run db:test:restart && npm run kafka:restart && dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "test:ci": "jest --coverage --runInBand --ci --reporters=default --reporters=jest-junit --reporters=github-actions --json --coverage --testLocationInResults --outputFile=report/coverage/jest.json --verbose",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/infra/postgres/configs.ts",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/infra/postgres/migration/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/infra/postgres/migration/$npm_config_name",
    "migration:run": "npm run build && npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "kafka:start": "docker compose up zookeeper -d && sleep 5 && docker compose up kafka -d && sleep 5",
    "kafka:rm": "docker compose rm kafka -s -f -v && docker compose rm zookeeper -s -f -v",
    "kafka:restart": "npm run kafka:rm && npm run kafka:start",
    "db:dev:rm": "docker compose rm dev-db -s -f -v && docker compose rm mongo-dev -s -f -v",
    "db:dev:up": "docker compose up dev-db -d && docker compose up mongo-dev -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 5 && npm run migration:run",
    "db:test:deploy": "dotenv -e .env.test -- npm run migration:run",
    "db:test:rm": "docker compose rm test-db -s -f -v && docker compose rm mongo-test -s -f -v",
    "db:test:up": "docker compose up test-db -d && docker compose up mongo-test -d",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run db:test:deploy",
    "redis:up": "docker compose up redis -d",
    "redis:rm": "docker compose rm redis -s -f -v",
    "redis:restart": "npm run redis:rm && npm run redis:up",
    "prepare-dev": "husky install",
    "greeting-proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. src/app/api/grpc/greeting/proto/*.proto --ts_proto_opt=addGrpcMetadata=true || protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=. src/app/api/grpc/greeting/proto/*.proto --ts_proto_opt=addGrpcMetadata=true",
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "^4.0.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@keyvhq/core": "^2.1.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@ocean-network-express/om-mdm-protobuf": "^0.0.71",
    "@songkeys/nestjs-redis": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-var-no-babel": "^1.2.0",
    "dayjs": "^1.11.10",
    "eslint-output": "^3.0.1",
    "google-auth-library": "^9.6.2",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "moment": "^2.29.4",
    "mongoose": "^7.6.3",
    "nestjs-cls": "^3.6.0",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "ts-proto": "^1.165.1",
    "typeorm": "^0.3.17",
    "v8n": "^1.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.20",
    "@types/jest": "29.5.6",
    "@types/node": "^18.16.0",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "sleep.cmd": "^2.0.23",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest-junit": {
    "outputDirectory": "./report/unit",
    "outputName": "test-report.xml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": [
      "npm run lint"
    ]
  }
}
